{
  "name": "haystack-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf public/bundles/ && rm -rf static_codegen",
    "codegen": "mkdir -p static_codegen && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./static_codegen/ --grpc_out=./static_codegen --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./haystack-idl/proto/  ./haystack-idl/proto/span.proto && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./static_codegen/ --grpc_out=./static_codegen --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./haystack-idl/proto/api/ -I ./haystack-idl/proto/ ./haystack-idl/proto/api/traceReader.proto",
    "coverage": "nyc --extension .jsx npm test && nyc report --reporter=text-lcov | coveralls",
    "eslint": "echo 'Running eslint...' && eslint --ext js,jsx,json --ignore-pattern 'public/*' --ignore-pattern 'haystack-idl/*' --ignore-pattern 'static_codegen/*' --ignore-pattern 'coverage/*' . && echo 'Done' ",
    "test": "npm run eslint && mocha --require ignore-styles --compilers js:babel-register --require ./test/src/test_helper.js \"test/**/*@(.js|.jsx)\" --exit --timeout 10000",
    "build": "npm run clean && npm run codegen && npm run coverage && webpack --mode=production",
    "start": "node server/start.js",
    "start:dev": "npm run clean && npm run codegen && npm run test && concurrently -r \"npm run watch\" \"wait-on public/bundles/report.html && npm run start:devserver\"",
    "start:devserver": "NODE_ENV=development nodemon --ignore 'public/*' server/start.js",
    "watch": "NODE_ENV=development webpack -w --mode=development"
  },
  "engines": {
    "node": ">=8.6.0",
    "npm": ">=5.0.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-winston": "^2.5.1",
    "finished": "^1.2.2",
    "flat": "^4.0.0",
    "google-protobuf": "^3.5.0",
    "grpc": "^1.13.0",
    "https": "^1.0.0",
    "lodash": "^4.17.10",
    "measured": "^1.1.0",
    "moment": "^2.22.1",
    "particles.js": "^2.0.0",
    "passport": "^0.4.0",
    "passport-saml": "^0.33.0",
    "pug": "^2.0.3",
    "q": "^1.5.1",
    "qs": "^6.5.2",
    "serve-favicon": "^2.5.0",
    "statsd-client": "^0.4.2",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "axios-mock-adapter": "^1.15.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "canvas": "^1.6.10",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "chart.js": "^2.7.2",
    "color-hash": "^1.0.3",
    "concurrently": "^3.6.0",
    "coveralls": "^3.0.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.8.1",
    "grpc-tools": "^1.6.6",
    "history": "^4.7.2",
    "ignore-styles": "^5.0.1",
    "js-cookie": "^2.2.0",
    "jsdom": "^11.10.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.4.0",
    "mobx": "^3.6.2",
    "mobx-react": "^4.4.3",
    "mobx-utils": "^3.2.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.3",
    "nyc": "^11.7.3",
    "progress-bar-webpack-plugin": "^1.11.0",
    "prop-types": "^15.6.1",
    "proxyquire": "^2.0.1",
    "react": "^16.3.2",
    "react-bootstrap-table": "^4.0.0-beta.9",
    "react-chartjs-2": "^2.7.2",
    "react-circular-progressbar": "^0.8.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.0",
    "react-hot-loader": "^4.1.3",
    "react-modal": "^3.4.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-sparklines": "^1.7.0",
    "react-test-renderer": "^16.3.2",
    "react-typist": "^2.0.4",
    "react-vis": "^1.9.3",
    "sinon": "^1.17.7",
    "style-loader": "^0.21.0",
    "supertest": "^3.0.0",
    "timeago.js": "^3.0.2",
    "vis": "^4.21.0",
    "vizceral": "^4.6.3",
    "vizceral-react": "^4.5.5",
    "wait-on": "^2.1.0",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^2.11.3",
    "webpack-command": "^0.4.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "browser": true,
      "mocha": true,
      "jquery": true
    },
    "extends": "airbnb",
    "plugins": [
      "json"
    ],
    "rules": {
      "indent": 0,
      "max-len": [
        "error",
        250
      ],
      "react/jsx-indent": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "style|bootstrap"
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "object-curly-spacing": [
        0,
        "never"
      ],
      "jsx-a11y/no-autofocus": 0,
      "react/jsx-indent-props": 0,
      "react/jsx-tag-spacing": 0,
      "react/forbid-prop-types": 0,
      "no-trailing-spaces": 0,
      "import/no-named-as-default": 0,
      "no-multi-spaces": 0
    }
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "build",
      "coverage",
      "haystack-idl",
      "public",
      "static_codegen",
      "server/stores/trends/stub/*",
      "server/stores/traces/stub/*"
    ]
  }
}
