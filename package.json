{
  "name": "haystack-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf public/bundles/ && rm -rf static_codegen",
    "codegen": "mkdir -p static_codegen && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./static_codegen/ --grpc_out=./static_codegen --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./haystack-idl/proto/  ./haystack-idl/proto/span.proto && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./static_codegen/ --grpc_out=./static_codegen --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./haystack-idl/proto/api/ -I ./haystack-idl/proto/ ./haystack-idl/proto/api/traceReader.proto",
    "eslint": "echo 'Running eslint...' && eslint --ext js,jsx,json --ignore-pattern 'public/*' --ignore-pattern 'static_codegen/*' . && echo 'Done' ",
    "test": "npm run eslint && mocha --require ignore-styles --compilers js:babel-core/register --require ./test/src/test_helper.js \"test/**/*@(.js|.jsx)\" ",
    "build": "npm run clean && npm run codegen && npm test && webpack -p",
    "start": "node server/start.js",
    "start:dev": "npm run clean && npm run codegen && npm test && webpack -d && nodemon --ignore 'public/*' server/start.js",
    "watch": "webpack -w -d --progress"
  },
  "engines": {
    "node": ">=8.1.2",
    "npm": ">=5.0.3"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.1",
    "express-winston": "^2.0.0",
    "grpc": "^1.6.0",
    "lodash": "^4.17.4",
    "moment": "^2.16.0",
    "pug": "^2.0.0-beta6",
    "q": "^1.5.0",
    "serve-favicon": "^2.3.2",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.2",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "google-protobuf": "^3.4.0",
    "grpc-tools": "^1.6.0",
    "history": "^4.6.3",
    "html-loader": "^0.4.5",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.0.0",
    "less": "^2.3.1",
    "less-loader": "^4.0.3",
    "lodash-webpack-plugin": "^0.11.3",
    "mobx": "^3.1.9",
    "mobx-react": "^4.2.1",
    "mobx-utils": "^3.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "progress-bar-webpack-plugin": "^1.9.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-addons-perf": "^15.4.2",
    "react-bootstrap-table": "^4.0.0-beta.9",
    "react-circular-progressbar": "^0.1.5",
    "react-copy-to-clipboard": "^5.0.0",
    "react-datetime": "^2.8.10",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-modal": "^2.2.4",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.5",
    "react-test-renderer": "^15.6.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.18.0",
    "supertest": "^3.0.0",
    "timeago.js": "^3.0.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-bundle-size-analyzer": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  }
}
